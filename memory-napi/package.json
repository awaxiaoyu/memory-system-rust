{
  "name": "@meiju/memory-native",
  "version": "0.1.0",
  "description": "Rust-powered memory system for Meiju",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "memory",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-pc-windows-msvc",
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lyf19990503/meiju1"
  },
  "license": "MIT"
}
